{
    "name": "oro/crm-dotmailer",
    "description": "OroCRM Dotmailer integration",
    "homepage": "https://github.com/laboro/OroCRMDotmailerBundle.git",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Oro, Inc",
            "homepage": "http://www.orocrm.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "OroCRM\\Bundle\\DotmailerBundle": "src/"
        }
    },
    "target-dir": "src/OroCRM/Bundle/DotmailerBundle",
    "repositories": [
        {
            "type": "composer",
            "url": "http://packagist.orocrm.com"
        }
    ],
    "require": {
        "php": ">=5.4.9",
        "oro/crm": "1.7-dev",
        "romanpitak/dotmailer-api-v2-client": "1.0.10"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
